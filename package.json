{
    "name": "apoia",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env DOTENV_CONFIG_QUIET=true next dev --turbopack -p 8081",
        "build": "npm run copy-lamejs && cross-env DOTENV_CONFIG_QUIET=true NODE_OPTIONS=\"--max-old-space-size=4096\" npx next-swagger-doc-cli next-swagger-doc.json && cross-env DOTENV_CONFIG_QUIET=true NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
        "start": "cross-env DOTENV_CONFIG_QUIET=true next start -p 8088",
        "lint": "next lint",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "check": "npm run lint && npm run typecheck",
        "knex": "ts-node -P tsconfig.knex.json node_modules/knex/bin/cli.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "copy-lamejs": "node -e \"require('fs').copyFileSync('node_modules/lamejs/lame.min.js', 'public/lame.min.js')\""
    },
    "dependencies": {
