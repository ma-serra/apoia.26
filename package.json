    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/lodash": "^4.17.21",
        "cross-env": "^10.1.0",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.1",
        "eslint-config-next": "15.4.10",
        "glob": "^13.0.0",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1",
            "\\.(md)$": "<rootDir>/txtTransformer.js"
        },
        "transform": {
            "\\.txt$": "<rootDir>/txtTransformer.js"
        },
        "moduleFileExtensions": [
            "js",
            "ts",
            "json",
            "txt"
        ],
        "testMatch": [
            "**/__tests__/**/*.+(test.ts|spec.ts)",
            "**/?(*.)+(spec|test).+(ts|tsx)"
        ]
    }
}
